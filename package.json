{
  "name": "eslint-config-coveo",
  "version": "0.1.3",
  "description": "Coveo's shareable eslint config",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "ava",
    "commit": "git add .; git cz; git push",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coveo/eslint-config-coveo.git"
  },
  "keywords": [
    "coveo",
    "code",
    "quality",
    "lint",
    "eslint",
    "validate",
    "checker",
    "check",
    "enforce",
    "verify",
    "hint",
    "style",
    "code",
    "style",
    "linter",
    "jshint",
    "jscs",
    "jslint"
  ],
  "author": "Coveo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coveo/eslint-config-coveo/issues"
  },
  "homepage": "https://github.com/coveo/eslint-config-coveo#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "conventional-changelog": "^1.1.0",
    "cz-conventional-changelog": "^1.1.6",
    "eslint": "^3.2.0",
    "is-plain-obj": "^1.1.0",
    "temp-write": "^2.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
